{
  "name": "chika-server",
  "version": "0.1.0",
  "description": "A delightful way to building a Node.js RESTful API Services with beautiful code written in TypeScript",
  "main": "src/app.ts",
  "scripts": {
    "start": "nps",
    "test": "npm start test",
    "build": "npm start build",
    "presetup": "yarn install",
    "setup": "npm start config && npm start setup.script",
    "typeorm": "nps config && node -r ts-node/register ./node_modules/typeorm/cli.js",
    "prettier": "prettier --write --config ./.prettierrc './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx,js,jsx}"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express",
    "boilerplate",
    "skeleton",
    "starter-kit",
    "w3tec.ch"
  ],
  "dependencies": {
    "aws-sdk": "^2.797.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "class-validator-jsonschema": "^1.3.0",
    "commander": "^4.1.0",
    "compose-middleware": "^5.0.1",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.5",
    "copyfiles": "^2.1.0",
    "cors": "^2.8.4",
    "db-migrate-mysql": "^2.1.1",
    "dotenv": "8.2.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-bearer-token": "^2.4.0",
    "faker": "^4.1.0",
    "fejl": "^2.5.1",
    "figlet": "^1.2.0",
    "glob": "^7.1.2",
    "helmet": "^3.9.0",
    "iconv-lite": "^0.6.1",
    "jsonfile": "5.0.0",
    "jsonwebtoken": "^8.5.1",
    "microframework-w3tec": "^0.6.3",
    "moment": "^2.24.0",
    "morgan": "^1.9.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql": "^2.16.0",
    "nocache": "^2.1.0",
    "nodemailer": "^6.4.14",
    "nodemailer-smtp-pool": "^2.8.3",
    "nodemon": "^2.0.2",
    "nps": "^5.9.12",
    "nps-utils": "^1.7.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.8.0",
    "routing-controllers-openapi": "^1.7.0",
    "serve-favicon": "^2.4.5",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "8.6.2",
    "tslint": "^6.0.0",
    "typedi": "0.8.0",
    "typeorm": "^0.2.22",
    "typeorm-seeding": "^1.2.0",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.6.3",
    "uuid": "^3.3.2",
    "winston": "3.2.1",
    "yenv": "^2.1.1"
  },
  "resolutions": {
    "**/event-stream": "^4.0.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/test/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/unit/lib/setup.ts"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.18",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.11.0",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.4",
    "@types/faker": "^4.1.9",
    "@types/figlet": "^1.2.0",
    "@types/helmet": "0.0.45",
    "@types/jest": "24.9.1",
    "@types/morgan": "^1.7.35",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.5.0",
    "@types/reflect-metadata": "0.1.0",
    "@types/serve-favicon": "^2.2.29",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "25.1.0",
    "lint-staged": "^10.1.2",
    "mock-express-request": "^0.2.0",
    "mock-express-response": "^0.2.1",
    "ncp": "^2.0.0",
    "nock": "11.7.2",
    "prettier": "^2.0.3",
    "sqlite3": "^4.0.0",
    "ts-jest": "25.0.0",
    "tsconfig-paths": "^3.9.0"
  }
}
